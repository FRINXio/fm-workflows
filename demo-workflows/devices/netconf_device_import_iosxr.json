{
      "netconf": {
        "netconf-node-topology:host": "sample-topology",
        "netconf-node-topology:port": "{{port}}",
        "netconf-node-topology:keepalive-delay": "10",
        "netconf-node-topology:max-connection-attempts": 1,
        "netconf-node-topology:connection-timeout-millis": "30000",
        "netconf-node-topology:default-request-timeout-millis": 60000,
        "netconf-node-topology:tcp-only": "false",
        "netconf-node-topology:username": "{{username}}",
        "netconf-node-topology:password": "{{password}}",
        "netconf-node-topology:sleep-factor": "1",
        "uniconfig-config:uniconfig-native-enabled": "true",
        "netconf-node-topology:edit-config-test-option": "set",
        "uniconfig-config:blacklist": {
          "uniconfig-config:path": [
            "openconfig-interfaces:interfaces",
            "openconfig-lacp:lacp",
            "ietf-interfaces:interfaces",
            "openconfig-vlan:vlans",
            "openconfig-routing-policy:routing-policy",
            "openconfig-lldp:lldp",
            "Cisco-IOS-XR-group-cfg:groups",
            "openconfig-acl:acl",
            "openconfig-network-instance:network-instances"
          ]
        },
        "node-extension:reconcile": "false",
        "netconf-node-topology:schema-cache-directory": "{{schema}}",
        "netconf-node-topology:between-attempts-timeout-millis": "10000",
        "netconf-node-topology:dry-run-journal-size": 150,
        "netconf-node-topology:enabled-notifications": false
      }
}